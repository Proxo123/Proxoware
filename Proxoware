revertNameButton.TextColor3 = Color3.fromRGB(255, 255, 255)
revertNameButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
revertNameButton.BackgroundTransparency = 0.5
revertNameButton.Font = Enum.Font.Gotham
revertNameButton.TextSize = 14
revertNameButton.BorderSizePixel = 0

local originalName = LocalPlayer.Name
local originalUserId = LocalPlayer.UserId

local function changeName(newName, newUserId)
    local playerName = LocalPlayer.Name
    local playerUserId = LocalPlayer.UserId

    local function change(item)
        if item:IsA("TextLabel") then
            if string.find(item.Text, playerName) then
                item.Text = string.gsub(item.Text, playerName, newName)
            end
            item:GetPropertyChangedSignal("Text"):Connect(function()
                if string.find(item.Text, playerName) then
                    item.Text = string.gsub(item.Text, playerName, newName)
                end
            end)
        elseif item:IsA("ImageLabel") then
            if string.find(item.Image, playerName) then
                item.Image = string.gsub(item.Image, playerName, newName)
            elseif string.find(item.Image, playerUserId) then
                item.Image = string.gsub(item.Image, playerUserId, newUserId)
            end
            item:GetPropertyChangedSignal("Image"):Connect(function()
                if string.find(item.Image, playerName) then
                    item.Image = string.gsub(item.Image, playerName, newName)
                elseif string.find(item.Image, playerUserId) then
                    item.Image = string.gsub(item.Image, playerUserId, newUserId)
                end
            end)
        elseif item:IsA("TextButton") then
            if string.find(item.Text, playerName) then
                item.Text = string.gsub(item.Text, playerName, newName)
            end
            item:GetPropertyChangedSignal("Text"):Connect(function()
                if string.find(item.Text, playerName) then
                    item.Text = string.gsub(item.Text, playerName, newName)
                end
            end)
        else
            return
        end
    end

    for _, v in game:GetDescendants() do
        change(v)
    end

    game.DescendantAdded:Connect(function(descendant)
        change(descendant)
    end)
end

local function setName()
    local newName = nameTextBox.Text
    local userIdUrl = "https://api.roblox.com/users/get-by-username?username=" .. newName
    local success, response = pcall(function()
        return HttpService:GetAsync(userIdUrl)
    end)

    if success then
        local data = HttpService:JSONDecode(response)
        if data.Id then
            changeName(newName, tostring(data.Id))
        else
            changeName(newName, originalUserId)
        end
    else
        changeName(newName, originalUserId)
    end
end

local function revertName()
    changeName(originalName, tostring(originalUserId))
end

setNameButton.MouseButton1Click:Connect(setName)
revertNameButton.MouseButton1Click:Connect(revertName)

